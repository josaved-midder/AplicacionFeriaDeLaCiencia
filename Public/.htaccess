# Enruta todo a index.php excepto archivos/carpetas reales
RewriteEngine On

# Cambia esta base si tu app no est√° exactamente en /AplicacionFeriaDeLaCiencia/Public
RewriteBase /AplicacionFeriaDeLaCiencia/Public/

RewriteCond %{REQUEST_FILENAME} !-f
